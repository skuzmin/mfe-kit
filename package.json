{
  "name": "mfe-kit",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "build": "rm -rf dist && node esbuild.config.js",
    "lint": "eslint src",
    "check": "prettier --ignore-path .gitignore --check src/** --log-level warn",
    "check:fix": "prettier --ignore-path .gitignore --write src/; eslint --fix src"
  },
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.3",
    "highlight.js": "^11.11.1",
    "hono": "^4.7.10",
    "js-yaml": "^4.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.21",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "^3.2.4"
  }
}
